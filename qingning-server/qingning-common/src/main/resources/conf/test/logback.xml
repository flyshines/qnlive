    </logger>

    <logger name="qingning.mq" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.mq.log" />
    </logger>

    <logger name="qingning" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.log" />
    </logger>

    <root level="debug">
        <appender-ref ref="console" />
    </root>

</configuration>