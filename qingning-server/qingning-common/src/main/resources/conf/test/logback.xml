    </appender>

    <appender name="qingning.common.db.log" class="ch.qos.logback.core.rolling.RollingFileAppender">
        <file>${qingning.common.db.log.path}</file>
        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">
