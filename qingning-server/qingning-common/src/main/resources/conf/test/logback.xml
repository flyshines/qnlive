    </appender>

    <logger name="qingning.db.common" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.db.common.log" />
    </logger>

    <logger name="qingning.common.server" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.common.server.log" />
    </logger>
    <logger name="qingning.common.db" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.common.db.log" />
    </logger>

    <logger name="qingning.lecture.server" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.lecture.server.log" />
    </logger>

    <logger name="qingning.saas.server" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.saas.server.log" />
    </logger>
    <logger name="qingning.saas.db" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.saas.db.log" />
    </logger>

    <logger name="qingning.lecture.db" level="DEBUG" additivity="false">
        <appender-ref ref="qingning.lecture.db.log" />
    </logger>

    <logger name="qingning.user.server" level="DEBUG" additivity="false">
