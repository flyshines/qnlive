<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="false" scan="true" scanPeriod="1 seconds">
    <timestamp key="bySecond" datePattern="yyyyMMdd'T'HHmmss"/>
    <contextName>logback</contextName>
    <property name="qingning.db.common.log.path" value="/usr/local/qingning-live/logs/qingning.db.common.log" />
    <property name="qingning.common.server.log.path" value="/usr/local/qingning-live/logs/qingning.common.server.log" />
    <property name="qingning.common.db.log.path" value="/usr/local/qingning-live/logs/qingning.common.db.log" />
    <property name="qingning.lecture.server.log.path" value="/usr/local/qingning-live/logs/qingning.lecture.server.log" />
    <property name="qingning.lecture.db.log.path" value="/usr/local/qingning-live/logs/qingning.lecture.db.log" />
    <property name="qingning.user.server.log.path" value="/usr/local/qingning-live/logs/qingning.user.server.log" />
    <property name="qingning.user.db.log.path" value="/usr/local/qingning-live/logs/qingning.user.db.log" />
    <property name="qingning.mq.log.path" value="/usr/local/qingning-live/logs/qingning.mq.log" />
    <property name="qingning.log.path" value="/usr/local/qingning-live/logs/qingning.log" />
    <property name="sys.log.path" value="/usr/local/qingning-live/logs/qingning.sys.log" />
    <property name="qingning.saas.server.log.path" value="/usr/local/qingning-live/logs/qingning.saas.server.log" />
    <property name="qingning.saas.db.log.path" value="/usr/local/qingning-live/logs/qingning.saas.db.log" />

    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">
            <level>DEBUG</level>
        </filter>
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %contextName [%thread] %-5level %logger{36} - %msg%n
            </pattern>
        </encoder>
    </appender>

    <appender name="sys.log" class="ch.qos.logback.core.rolling.RollingFileAppender">
        <file>${sys.log.path}</file>
        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">
            <level>DEBUG</level>
        </filter>
        <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
            <fileNamePattern>${sys.log.path}.%d{yyyy-MM-dd}.zip</fileNamePattern>
        </rollingPolicy>
        <encoder>
            <pattern>%date %level [%thread] %logger{36} [%file : %line] %msg%n</pattern>
        </encoder>
    </appender>

    <appender name="qingning.log" class="ch.qos.logback.core.rolling.RollingFileAppender">
        <file>${qingning.log.path}</file>
        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">
            <level>DEBUG</level>
        </filter>
        <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
            <fileNamePattern>${qingning.log.path}.%d{yyyy-MM-dd}.zip</fileNamePattern>
        </rollingPolicy>
        <encoder>
            <pattern>%date %level [%thread] %logger{36} [%file : %line] %msg%n</pattern>
        </encoder>
    </appender>

    <appender name="qingning.mq.log" class="ch.qos.logback.core.rolling.RollingFileAppender">
        <file>${qingning.mq.log.path}</file>
        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">
            <level>DEBUG</level>
        </filter>
        <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
            <fileNamePattern>${qingning.mq.log.path}.%d{yyyy-MM-dd}.zip</fileNamePattern>
        </rollingPolicy>
        <encoder>
            <pattern>%date %level [%thread] %logger{36} [%file : %line] %msg%n</pattern>
        </encoder>
    </appender>

    <appender name="qingning.user.db.log" class="ch.qos.logback.core.rolling.RollingFileAppender">
