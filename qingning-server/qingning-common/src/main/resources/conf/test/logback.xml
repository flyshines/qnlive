        </filter>
        <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
            <fileNamePattern>${qingning.common.db.log.path}.%d{yyyy-MM-dd}.zip</fileNamePattern>
        </rollingPolicy>
